{
	"name": "vscode-js-template-literal",
	"displayName": "vscode-js-template-literal",
	"description": "Syntax highlighting for javascript in template literals",
	"version": "0.1.0",
	"publisher": "icanhasjonas",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/icanhasjonas/vscode-js-template-literal.git"
	},
	"engines": {
		"vscode": "^1.5.0"
	},
	"categories": [
		"Languages"
	],
	"contributes": {
		"grammars": [
			{
				"injectTo": [
					"source.js",
					"source.jsx",
					"source.ts",
					"source.tsx"
				],
				"scopeName": "inline.lit-js",
				"path": "./syntaxes/lit-js.json",
				"embeddedLanguages": {
					"meta.embedded.block.js": "js"
				}
			}
		]
	}
}
